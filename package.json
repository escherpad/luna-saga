    "Angular2",
    "ng2",
    "TypeScript",
    "ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/escherpad/luna-saga.git"
  },
  "bugs": {
    "url": "https://github.com/escherpad/luna-saga/issues"
  },
  "scripts": {
    "postinstall": "npm prune && typings install",
    "test": "karma start",
    "clean": "rimraf dist",
