    "rxjs": "^5.0.2"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^3.0.0-beta.17",
    "browserify": "^12.0.1",
    "es6-shim": "^0.35.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
